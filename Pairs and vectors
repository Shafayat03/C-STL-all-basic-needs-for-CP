#include<bits/stdc++.h>
using namespace std;

#define int long long
#define endl '\n'
typedef long long ll;


int32_t main()
{

    /// declaring a pair

    pair<int , string > p;

    /// process 1 for making pair

   p =  make_pair(2,"Shafu");

   cout << p.first << " " << p.second << endl;
        /// 2              /// Shafu


        /// process 2  declaring

      //  p  = {2, "abcd"};


   /// coping pair to another pair

   pair<int, string > p1 = p;

   p1.first = 3;

   cout << p1.first << " " << p1.second << endl;

   /// 2i tah array r modde 1sathe operation chalanur jono amara pair use kori
   /// dui tah array er modde ek sathe operation chalaite chaitle

   pair<int,int> p_array[3];
      p_array[0] = {1,2};
      p_array[1] = {3,6};
      p_array[2] = {2, 7};
swap(p_array[0], p_array[2]);
      for(int i = 0; i <  3; i++)
      {
          cout << p_array[i].first << " " << p_array[i].second << endl;
       }
    return 0;
}
